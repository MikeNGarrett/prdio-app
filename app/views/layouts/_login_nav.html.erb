						<% if admin_signed_in? -%>
							<li><%= link_to(edit_admin_registration_path) do %>
								<%= current_admin.email %>
								<i class="foundicon-settings"></i>
								<% end %>
							</li>
							<li><%= link_to current_admin.email, admins_path %>Administration</li>
